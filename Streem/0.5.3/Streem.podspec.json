    "StreemShared": [
      "Streem/StreemShared/**/*.{xcassets,scnassets}",
      "Streem/StreemShared/**/**/*.{storyboard,xib}"
    ],
    "StreemJob": [
      "Streem/StreemJob/Assets/*.scnassets"
